{
  "name": "intergram",
  "version": "1.0.0",
  "description": "A live chat widget linked to your telegram messenger",
  "scripts": {
    "start": "node server.js",
    "dev": "start http://localhost:3000/demo.html & node devServer.js",
    "build": "shx mkdir -p dist && shx cp static/*.html dist && shx cp -r static/media dist && shx cp -r static/css dist && webpack -p",
    "test": "npm run -s lint",
    "lint": "eslint src"
  },
  "authors": [
    "Ido Cohen (http://github.com/idoco)"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.0",
    "eslint": "^5.5.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-preact": "^1.1.6",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "webpack": "^4.17.1",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.11"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "dateformat": "^3.0.3",
    "express": "^4.16.4",
    "human-readable-ids": "^1.0.4",
    "preact": "^8.3.1",
    "request": "^2.79.0",
    "shx": "^0.3.2",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "store2": "^2.7.0",
    "whatwg-fetch": "^3.0.0"
  }
}
